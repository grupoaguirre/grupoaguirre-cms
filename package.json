{
  "name": "aguirre-server",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation npm run generate:types && next build",
    "build:test": "cross-env NODE_OPTIONS=--no-deprecation npm run generate:types && next lint",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:lexical": "cross-env NODE_OPTIONS=--no-deprecation payload generate:lexical",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "next start -p 10000"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@payloadcms/db-mongodb": "3.52.0",
    "@payloadcms/email-nodemailer": "3.52.0",
    "@payloadcms/next": "3.52.0",
    "@payloadcms/payload-cloud": "3.52.0",
    "@payloadcms/plugin-cloud-storage": "3.52.0",
    "@payloadcms/richtext-lexical": "3.52.0",
    "@payloadcms/storage-s3": "3.52.0",
    "@payloadcms/ui": "3.52.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "axios": "^1.10.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "graphql": "^16.8.1",
    "isomorphic.js": "^0.2.5",
    "lib0": "^0.2.109",
    "monaco-editor": "^0.52.2",
    "next": "^15.4.6",
    "payload": "^3.52.0",
    "picomatch": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "typescript": "5.7.3",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/axios": "^0.9.36",
    "@types/dotenv": "^6.1.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
